import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      // write your code here  
      Scanner sc = new Scanner(System.in);
      int n,rev=0;
      
      n=sc.nextInt();
      while(n!=0)
      {
          int num1=n%10;
          rev=rev*10+num1;
          n=n/10;
      }
      while(rev!=0)
      {
          int num2=rev%10;
          System.out.println(num2);
          rev=rev/10;
      }
     }
    }
