import java.util.*;
  
  public class Main{
  
  public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
    int N=scn.nextInt();
    int i=1;
    while(i<=N)
    {
        if(i%2==0)
        {
            System.out.println( i + " is even");
        }
        else
        {
            System.out.println( i  + " is odd");
}
        i++;
}
    System.out.println("ODD EVEN CLASSIFIED");
}
   
  }
