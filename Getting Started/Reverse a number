import java.util.*;
   
   public class Main{
   
   public static void main(String[] args) {
     // write your code here  
     Scanner sc = new Scanner(System.in);
     int n=sc.nextInt();
     int rev=0;
     while(n>0)
     {
         int num=n%10;
         rev=rev*10+num;
         n=n/10;
         System.out.println(rev);
         rev=0;
     }
    }
   }
